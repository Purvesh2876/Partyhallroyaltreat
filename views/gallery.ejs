<script>
    // Add event listeners to the filter buttons
    var filterButtons = document.querySelectorAll('.filterButton');

    filterButtons.forEach(function (button) {
        button.addEventListener('click', function () {
            // Hide all images
            var galleryBlocks = document.querySelectorAll('.gallery-block');
            galleryBlocks.forEach(function (block) {
                block.style.display = 'none';
            });

            // Show all images if the "All" button is clicked
            if (button.getAttribute('data-tag') === 'all') {
                galleryBlocks.forEach(function (block) {
                    block.style.display = 'block';
                });
            } else {
                // Show only images with the selected tag
                var selectedTag = button.getAttribute('data-tag');
                var filteredImages = document.querySelectorAll('.gallery-block[data-tag="' + selectedTag + '"]');
                filteredImages.forEach(function (image) {
                    image.style.display = 'block';
                });
            }
        });
    });
</script>


